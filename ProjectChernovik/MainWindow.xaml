<Window x:Class="ProjectChernovik.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProjectChernovik"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" MinHeight="500" MinWidth="820">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="70"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
        </Grid.RowDefinitions>
        <Grid Grid.Row="0" Background="#FEA9AB">
            <Image Source="Resources\Черновик.png" VerticalAlignment="Center" HorizontalAlignment="Left" Margin="5"></Image>
            <TextBlock Text="Chernovik" VerticalAlignment="Center" HorizontalAlignment="Center" FontFamily="Segoe Print" FontSize="25"></TextBlock>
        </Grid>
        <Frame Name="frmMain" Grid.Row="1" NavigationUIVisibility="Hidden"></Frame>
    </Grid>
</Window>
